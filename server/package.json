{
  "name": "promptdepot-server",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "install:all": "NODE_ENV=development npm install && NODE_ENV=production",
    "prod": "npm run install:all && npm run lint && npm run db:deploy && npm run build",
    "staging": "npm install && npm run lint && npm run db:deploy && npm run build",
    "dev": "npm i && npm run db:deploy && npm run nodemon",
    "dev:server": "npm run build && npm run start",
    "db:deploy": "npm run db:migrate && npm run db:seed",
    "db:deploy:fresh": "CLEAR_DB=true npm run db:deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:new": "drizzle-kit generate --name ",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:fresh": "CLEAR_DB=true npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "nodemon": "nodemon --config nodemon.json",
    "debug": "nodemon --config nodemon.json --inspect",
    "server": "npm run build && npm run start",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "ENV_FILE=./config/.env.test jest",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.36.4",
    "fastify": "^5.3.3",
    "fastify-plugin": "^5.0.1",
    "humanloop": "^0.8.20",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "pino-seq": "^2.1.0",
    "postgres": "^3.4.5",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "drizzle-kit": "^0.29.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
}
