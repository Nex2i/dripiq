{
  "name": "dripiq-server",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "install:all": "NODE_ENV=development npm install && NODE_ENV=production",
    "prod:server": "npm run install:all && npm run lint && npm run db:deploy && npm run build",
    "prod:workers": "npm run install:all && npm run lint && npm run build",
    "staging": "npm run install:all && npm run lint && npm run db:deploy && npm run build",
    "dev": "npm run install:all && npm run clear:dist && npm run db:deploy && npm run nodemon",
    "clear:dist": "rm -rf dist",
    "dev:server": "npm run build && npm run start",
    "db:deploy": "npm run db:migrate && npm run db:seed",
    "db:deploy:fresh": "CLEAR_DB=true npm run db:deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:new": "drizzle-kit generate --name ",
    "db:seed": "tsx src/db/seed.ts",
    "db:seed:fresh": "CLEAR_DB=true npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "nodemon": "nodemon --config nodemon.json",
    "debug": "nodemon --config nodemon.json --inspect",
    "server": "npm run build && npm run start",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "ENV_FILE=./config/.env.test jest",
    "test:clear": "ENV_FILE=./config/.env.test jest --clearCache",
    "test:clean": "npm run test:clear && npm run test",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "workers": "node dist/workers/worker.run.js",
    "dev:workers": "tsx src/workers/worker.run.ts",
    "dev:prepare": "npm run install:all && npm run clear:dist && npm run db:deploy",
    "dev:both": "npm run dev:prepare && concurrently -n server,workers -c green,cyan --kill-others-on-fail \"npm run nodemon\" \"npm run dev:workers\"",
    "start:both": "npm run build && concurrently -n server,workers -c green,cyan --kill-others-on-fail \"npm run start\" \"npm run workers\"",
    "check": "prettier --write . && eslint --fix"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/fastify": "^6.13.0",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@googleapis/gmail": "^14.0.1",
    "@highlight-run/node": "^3.12.22",
    "@highlight-run/pino": "^0.1.6",
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "@langfuse/client": "^4.2.0",
    "@langfuse/core": "^4.2.0",
    "@langfuse/langchain": "^4.2.0",
    "@langfuse/otel": "^4.2.0",
    "@langfuse/tracing": "^4.2.0",
    "@mendable/firecrawl-js": "^4.3.7",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@opentelemetry/auto-instrumentations-node": "^0.64.6",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sendgrid/client": "^8.1.5",
    "@sendgrid/eventwebhook": "^8.0.0",
    "@sendgrid/mail": "^8.1.5",
    "@sinclair/typebox": "^0.34.41",
    "@supabase/supabase-js": "^2.58.0",
    "awesome-phonenumber": "^7.5.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.60.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.6",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "ioredis": "^5.8.0",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.35",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "postgres": "^3.4.7",
    "socket.io": "^4.8.1",
    "string-similarity": "^4.0.4",
    "uuid": "^11.1.0",
    "zod": "^4.1.11"
  },
  "overrides": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.21",
    "@types/string-similarity": "^4.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "concurrently": "9.2.1",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
